@article{Yammine2019timeDependence,
  abstract = {<jats:p>We report a time-resolved measurement of the full transmission matrix (TM) of a short length of specialty annular-core few-mode fiber which guides 10 vector modes. We show how our method can isolate the fiber TM from \textquotedblleft{}misalignment\textquotedblright{} contributions from optics upstream and downstream of the fiber. From measurements spanning two days, we extract the drift of the fiber TM. We show that drifts in the TM elements are mostly described as correlated phase variations rather than amplitude variations. We show that an empirical model of the fiber TM parametrized in one parameter can successfully account for the drift.</jats:p>},
  author = {Yammine, J. and Tandj\`{e}, A. and Dossou, Michel and Bigot, L. and Andresen, E. R.},
  doc_url = {https://pubs.aip.org/aip/app/article-pdf/doi/10.1063/1.5047578/14568874/022904\_1\_online.pdf},
