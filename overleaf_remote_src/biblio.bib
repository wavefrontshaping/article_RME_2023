@article{Mazumder2004analysis,
  abstract = {<jats:p>We have systematically analyzed the excess scattering in a relatively large index optical fiber operating in the single mode regime. A mathematical model based on the theory of scattering of plane waves from a randomly perturbed core-clad interface is presented that predicts excess scattering confined within a small angle in the forward direction. An experimental system is developed that can measure the angular distribution of the scattering over \sim{}0\textdegree{}\textendash{}180\textdegree{}. Excellent agreement between computed and measured scattering distribution is observed over multiple wavelengths and wide angular range. The spectral and angular distribution of the excess scattering and its response to the perturbation parameters are analyzed.</jats:p>},
  author = {Mazumder, P. and Logunov, S.~L. and Raghavan, S.},
  doc_url = {https://pubs.aip.org/aip/jap/article-pdf/96/8/4042/14678174/4042\_1\_online.pdf},
  doi = {10.1063/1.1786378},
  files = {['mazumder2004analysis.pdf']},
  issue = {8},
  journal = {Journal of Applied Physics},
  language = {en},
  month = {10},
  pages = {4042--4049},
  publisher = {AIP Publishing},
  title = {Analysis of excess scattering in optical fibers},
  url = {http://dx.doi.org/10.1063/1.1786378},
  volume = {96},
  year = {2004},
}

